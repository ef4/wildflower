

<span class="dropdown-label">
  {{selectedStudentAge.value}}
</span>

<ul class="dropdown-list student-age-dropdown">
  {{#each ageRange in studentAgeRanges}}
    <li class="dropdown-item" {{action 'selectStudentAge' ageRange}}>
      <div class="dropdown-option">
        {{ ageRange.value }}
      </div>
    </li>
  {{/each}}
</ul>