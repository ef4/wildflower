          {
            "name": "Infant",
            "definition": "A child who is younger than 15 months old",
            "reference": "606 CMR 7.02",
            "referenceURL": "http://www.mass.gov/courts/docs/lawlib/600-699cmr/606cmr7.pdf"
          },
          {
            "name": "Toddler",
            "definition": "A child who is at least 15 months of age, but younger than 33 months old",
            "reference": "606 CMR 7.02",
            "referenceURL": "http://www.mass.gov/courts/docs/lawlib/600-699cmr/606cmr7.pdf"
          },
          {
            "name": "Preschool",
            "definition": "Any child at least two years and nine months old but not yet attending first grade",
            "reference": "606 CMR 7.02",
            "referenceURL": "http://www.mass.gov/courts/docs/lawlib/600-699cmr/606cmr7.pdf"
          },
          {
            "name": "Kindergarten",
            "definition": "A child who is five years old or who will attend first grade the following year in a public or private school",
            "reference": "606 CMR 7.02",
            "referenceURL": "http://www.mass.gov/courts/docs/lawlib/600-699cmr/606cmr7.pdf"
          },
          {
            "name": "School Age",
            "definition": "A kindergarten child, or a child who is attending a public or approved private elementary school",
            "reference": "606 CMR 7.02",
            "referenceURL": "http://www.mass.gov/courts/docs/lawlib/600-699cmr/606cmr7.pdf"
          }
